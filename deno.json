{
  "lock": false,
  "tasks": {
    "add": "cd pack && ./packwiz curseforge add",
    "refresh": "cd pack && ./packwiz refresh",
    "dev": "cd pack && ./packwiz serve",
    "update": "cd pack && ./packwiz update",
    "update:all": "cd pack && ./packwiz update --all",
    "install:datapack": "deno run -A scripts/installDatapack.ts",
    "inspect": "deno run -A scripts/inspectMod.ts",
    "instance:open": "deno run -A scripts/openInstance.ts"
  }
}
